<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.airlc.mapper.dustMapper">
 
    <!-- 
        mybatis-config.xml의 typeAlias에 등록해두지 않았다면 
        
        <select id="selectMember" resultType="com.airlc.dto.dustVO" >
            SELECT * FROM dust
        </select>
        
        이렇게 해야 한다.
    -->
    
    <select id="selectDust" resultType="com.airlc.dto.DustVO">
        SELECT * FROM dust
    </select>
    <select id="LatestDust" resultType="com.airlc.dto.DustVO">
        SELECT * FROM dust order by date desc limit 1
    </select>
    
</mapper>