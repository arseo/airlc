<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.airlc.mapper.tempMapper">
 
    <!-- 
        mybatis-config.xml의 typeAlias에 등록해두지 않았다면 
        
        <select id="selectMember" resultType="com.airlc.dto.tempVO" >
            SELECT * FROM temp
        </select>
        
        이렇게 해야 한다.
    -->
    
    <select id="selectTemp" resultType="com.airlc.dto.TempVO">
        SELECT * FROM temp
    </select>
    <select id="LatestTemp" resultType="com.airlc.dto.TempVO">
        SELECT * FROM temp order by date desc limit 1
    </select>
    
</mapper>